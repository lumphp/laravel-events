{
    "name": "lumphp/laravel-events",
    "description": "The Illuminate Events package.",
    "license": "MIT",
    "homepage": "https://laravel.com",
    "support": {
        "issues": "https://github.com/lumphp/laravel-events/issues",
        "source": "https://github.com/lumphp/laravel-events"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/bus": "^8.0",
        "illuminate/collections": "^8.0",
        "illuminate/macroable": "^8.0",
        "lumphp/laravel-container": "^7.0",
        "lumphp/laravel-contracts": "^7.0",
        "lumphp/laravel-support": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Illuminate\\Events\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "8.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
